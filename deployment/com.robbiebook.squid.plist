<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.robbiebook.squid</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/opt/squid/sbin/squid</string>
        <string>-N</string>
        <string>-f</string>
        <string>/usr/local/etc/squid/squid.conf</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/Users/allanperetz/aurora-ai-robbiverse/logs/squid-stdout.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/allanperetz/aurora-ai-robbiverse/logs/squid-stderr.log</string>
    
    <key>WorkingDirectory</key>
    <string>/usr/local/var</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
    
    <key>Nice</key>
    <integer>-5</integer>
</dict>
</plist>

